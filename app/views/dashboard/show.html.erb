<h1>Welcome, <%= current_user.username %></h1>

<h2>Private Repos</h2>
<% @data.each do |repo| %>
<ul>
  <% if repo[:private] == true %>
    <li><%= repo[:name] %></li>
    <% end %>
    </ul>
<% end %>

<h2>Public Repos</h2>
<% @data.each do |repo| %>
    <ul>
      <% if repo[:private] == false %>
      <li><%= repo[:name] %></li>
      <% end %>
    </ul>
<% end %>
